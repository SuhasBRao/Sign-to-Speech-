reloaded = tf.keras.models.load_model('C:/Users/suhas/Downloads/best_model_ISL.h5') # this loads the model saved in my                                                                                      laptop at specified location
img = cv2.imread('Foreground.jpg')
img = cv2.resize(img,(150,150))
cv2.imshow('Input',img)
cv2.waitKey(0)
cv2.destroyAllWindows()
# reshape the img captured to feed it to the model
img = np.reshape(img,(1,img.shape[0], img.shape[1],-1))
pred = reloaded.predict(img)
val = np.argmax(pred)
print(val)
